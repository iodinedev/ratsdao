generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URI")
}

model Hashes {
  hash      String   @id
  timestamp DateTime @default(now())
}

model Nfts {
  id          Int       @id
  name        String
  details     String
  project     String
  status      String
  imagePath   String    @unique
  tokenamount Int
  price       Int
  selldate    Int
}
